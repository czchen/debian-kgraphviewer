#!/usr/bin/make -f

include /usr/share/pkg-kde-tools/qt-kde-team/2/debian-qt-kde.mk

override_dh_auto_build:
	docbook-to-man debian/kgraphviewer.sgml > debian/kgraphviewer.1
	dh_auto_build

override_dh_auto_install:
	dh_auto_install
	mv $(CURDIR)/debian/tmp/usr/lib/libkgraphviewer.so.2.1 $(CURDIR)/debian/tmp/usr/lib/libkgraphviewer.so.3

override_dh_strip:
	dh_strip --dbg-package=kgraphviewer-dbg
